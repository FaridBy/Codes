#include <stdlib.h>
#include <stdio.h>
#include <locale.h>
int main()
{setlocale(LC_ALL, "Turkish");
	int a,i,t;
	int toplam;
	printf("Lütfen sayýyý giriniz:\n");
scanf("%d",&t);
for(a; a<t;a++){
	toplam=0;
		for(i=1;i<a;i++)
		{if(a%i==0)
			toplam=toplam+i;
	
			}
				if(toplam==a)
			printf("%d mükemmel sayýdýr.\n",a);}
}
	
